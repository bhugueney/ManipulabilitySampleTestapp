#include "SimpleSampleVisitor.h"
#include "kinematic/Sample.h"

SimpleSampleVisitor::SimpleSampleVisitor(Tree* pTree)
	: SampleGeneratorVisitor_ABC()
{
	pTree_ = pTree;
}

SimpleSampleVisitor::~SimpleSampleVisitor()
{
	// NOTHING
}

void SimpleSampleVisitor::Visit(const Sample& sample)
{
	sample.LoadIntoTree(*pTree_);
}
